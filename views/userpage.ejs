<section>
    <div class="container">
        <% if (!ppdvideoinfo || !ppdvideoinfo[0]) { %>
        <h1 class="errorMessage">You have watched all your videos for this category!</h1>
        <% } else{  %>
        <div class="row">
            <div class="card">
                <iframe class="card-img-top" id="ppdvideo"
                    src="https://www.youtube.com/embed/<%=ppdvideoinfo[0].id.videoId%>"></iframe>
                <div class="card-body">
                    <div class="card-title">
                        <%=ppdvideoinfo[0].snippet.title%>
                    </div>
                    <p class="card-text">
                        <%=ppdvideoinfo[0].snippet.description%>
                    </p>
                    <p class="card-text">from channel:
                        <%=ppdvideoinfo[0].snippet.channelTitle%>
                    </p>
                    <button data-id="<%=ppdvideoinfo[0].id.videoId%>" class="videowatched btn btn-primary"
